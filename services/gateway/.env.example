# Gateway Service Environment Variables
# Copy to .env and customize

# Server Configuration
PORT=8080
NODE_ENV=development
LOG_LEVEL=info

# Security
WEBHOOK_SECRET=dev-change-me-secure-secret
CORS_ORIGIN=*

# Orchestrator Integration
ORCHESTRATOR_URL=http://orchestrator:7071/api/pr-comment

# Developer ID (for multi-dev environments)
DEV_ID=dev

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Timeouts
HTTP_TIMEOUT=30000
ORCHESTRATOR_TIMEOUT=60000
