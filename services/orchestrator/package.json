{
  "name": "code-agent-orchestrator",
  "version": "1.0.0",
  "description": "Durable Functions orchestrator for Code Agent MVP",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "func start",
    "start:host": "func start --typescript",
    "dev": "func start --typescript --verbose --cors *",
    "dev:watch": "npm run build && func start --typescript --verbose --cors *",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/functions": "^4.2.0",
    "durable-functions": "^3.0.4",
    "zod": "^3.22.4",
    "pino": "^8.16.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2",
    "azure-functions-core-tools": "^4.0.5455"
  },
  "keywords": [
    "azure-functions",
    "durable-functions",
    "orchestrator",
    "typescript"
  ],
  "author": "Code Agent Team",
  "license": "MIT"
}
